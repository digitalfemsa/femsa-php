{
    "name": "femsa/femsa-php",
    "version": "6.0.4",
    "description": "Femsa PHP Library",
    "type": "library",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api",
        "femsa"
    ],
    "homepage": "https://www.femsa.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "engineering@femsa.com",
            "homepage": "https://github.com/femsa/femsa-php/contributors"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5",
        "phpstan/phpstan": "1.10.47"
    },
    "autoload": {
        "psr-4": { "Femsa\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Femsa\\Test\\" : "test/" }
    }
}
